<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="trans">
	<insert id="insertUserInfo">
		insert into user_info(
		user_name,pass_word
		)values(
		#{userName},
		#{passWord}
		)
	</insert>
</mapper>
